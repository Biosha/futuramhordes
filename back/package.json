{
   "name": "back",
   "version": "1.0.0",
   "description": "Futuramhordes API",
   "main": "src/server.ts",
   "type": "module",
   "scripts": {
      "serve": "yarn run prettier && concurrently -k -n \"Typescript,Node\" -p \"[{name}]\" -c \"blue,green\" \"tsc --watch\" \"nodemon dist/server.js\"",
      "debug": "concurrently -k -n \"Typescript,Node\" -p \"[{name}]\" -c \"blue,green\" \"tsc --watch\" \"node --inspect dist/server.js\"",
      "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write",
      "test:ci": "tsc && NODE_ENV=development jest --config ./jest.config.json --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --reporters=jest-junit --watchAll=false",
      "test:debug": "tsc && NODE_ENV=development jest --config ./jest.config.json --watchAll",
      "build": "tsc",
      "start": "ts-node src/index.ts",
      "typeorm": "typeorm-ts-node-esm",
      "run:migration": "tsc && typeorm-ts-node-esm migration:run -d dist/data-source.js"
   },
   "keywords": [
      "nodejs",
      "express",
      "sequelize",
      "postgresql",
      "rest",
      "api"
   ],
   "author": "Biosha",
   "license": "ISC",
   "dependencies": {
      "body-parser": "^1.19.0",
      "cors": "^2.8.5",
      "cron": "^1.8.2",
      "express": "^4.17.1",
      "express-jwt": "^6.0.0",
      "express-validator": "^6.13.0",
      "jsonwebtoken": "^8.5.1",
      "multer": "^1.4.4-lts.1",
      "pg": "^8.4.0",
      "pg-hstore": "^2.3.3",
      "reflect-metadata": "^0.1.13",
      "request": "^2.88.2",
      "toml": "^3.0.0",
      "typeorm": "0.3.7",
      "url": "^0.11.0"
   },
   "devDependencies": {
      "@babel/cli": "^7.14.8",
      "@babel/preset-env": "^7.14.8",
      "@types/cheerio": "^0.22.29",
      "@types/cors": "^2.8.10",
      "@types/cron": "^1.7.2",
      "@types/express": "^4.17.11",
      "@types/express-jwt": "^6.0.1",
      "@types/jest": "^26.0.22",
      "@types/lodash": "^4.14.168",
      "@types/multer": "^1.4.7",
      "@types/node": "^16.11.10",
      "@types/request": "^2.48.5",
      "@types/validator": "^13.1.3",
      "babel-jest": "^26.6.3",
      "concurrently": "^6.0.0",
      "jest": "^26.6.3",
      "jest-junit": "^12.2.0",
      "nodemon": "^2.0.7",
      "prettier": "2.2.1",
      "ts-jest": "^26.5.5",
      "ts-node": "10.9.1",
      "typescript": "4.7.4"
   },
   "packageManager": "yarn@3.1.0"
}
